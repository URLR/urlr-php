{
    "name": "urlr/urlr-php",
    "version": "2.4.0",
    "description": "PHP client for URLR",
    "keywords": ["link shortener", "api", "sdk", "client"],
    "homepage": "https://urlr.me/en",
    "license": "MIT",
    "authors": [
        {
            "name": "URLR",
            "homepage": "https://urlr.me/en"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "overtrue/phplint": "^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": { "URLR\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "URLR\\Test\\" : "tests/" }
    },
    "scripts": {
        "test": [
            "@phplint"
        ],
        "phplint": "phplint"
    }
}